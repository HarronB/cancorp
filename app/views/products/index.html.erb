<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>




<h2 class="text-center"> List of Products </h2>


<h3 class="text-center">Products for Sale</h3>

<div class="row">
  <div class="col-xs-6 col-xs-offset-3">
    <% @products.each do |product| %>
      <%= render "product_row", product: product, order_product: @order_product,  locals: {show_links: false} %>
    <% end %>
  </div>
</div>


  <%= link_to_next_page @products, 'Next Page' %>
<%= paginate @products %>


